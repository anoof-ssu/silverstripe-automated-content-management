{
  "name": "sunnysideup/automated-content-management",
  "description": "Use AI / LLM (like chatGPT) to manage ur content.",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "chatgpt",
    "lama",
    "llama",
    "claude",
    "ai",
    "llm",
    "content management",
    "cms",
    "content editor",
    "automation",
    "automated"
  ],
  "license": "BSD-3-Clause",
  "require": {
    "silverstripe/framework": "^5.0",
    "silverstripe/admin": "^2.0"
  },
  "require-dev": {
    "sunnysideup/sswebpack_engine_only": "dev-master",
    "silverleague/ideannotator": "*",
    "phpunit/phpunit": "^9.6",
    "squizlabs/php_codesniffer": "^3.7",
    "silverstripe/standards": "^1",
    "phpstan/extension-installer": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Sunnysideup\\AutomatedContentManagement\\": "src/"
    }
  },
  "extra": {
    "expose": [
      "client/dist"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "bin/npm-build-automated-content-management.sh",
    "bin/npm-watch-automated-content-management.sh"
  ],
  "authors": [
    {
      "name": "Nicolaas Thiemen Francken",
      "email": "modules@sunnysideup.co.nz",
      "role": "lead developer"
    }
  ]
}
